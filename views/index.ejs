    <script>


        $(document).ready(function () {
          $("#owl-demo").owlCarousel({

            navigation: true, // Show next and prev buttons
            navigationText: true,
            slideSpeed: false,
            paginationSpeed: false,

            items: 1,
            itemsDesktop: false,
            itemsDesktopSmall: false,
            itemsTablet: false,
            itemsMobile: false,
            paginationNumbers: true
          });
          $("#owl-demo-2").owlCarousel({
            pagination: false,
            dots: false,
            loop: true,
            autoplay: true,
            items: 1,
            itemsDesktop: false,
            itemsDesktopSmall: false,
            itemsTablet: false,
            itemsMobile: false,
          });
        });
    </script>
    <script type="text/javascript">
      var frm = $('#answer-form');
      frm.submit(function (e) {
        e.preventDefault();
        $.ajax({
          type: frm.attr('method'),
          url: frm.attr('action'),
          data: frm.serialize(),
          success: function (data) {
            if (data.isCorrect) {
              new Noty({
                theme: 'metroui',
                text: `${data.message}`,
                type: 'success',
                layout: 'center',
                timeout: 2500,
                modal: true,
                closeWith: ['click']
              }).show();
              setInterval(() => {
                location.reload()
              }, 2800)
            } else {
              new Noty({
                theme: 'metroui',
                text: `${data.message}`,
                type: 'error',
                layout: 'center',
                timeout: 2500,
                modal: true,
                closeWith: ['click']
              }).show();
            }
          },
          error: function (data) {
            console.log('An error occurred.');
            console.log(data);
            new Noty({
              theme: 'metroui',
              text: `You have been disqualified !`,
              type: 'error',
              layout: 'center',
              timeout: 2500,
              modal: true,
              closeWith: ['click']
            }).show();
          },
        });
      });
    </script>
